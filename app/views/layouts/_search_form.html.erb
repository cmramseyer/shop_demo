<%= form_with model: search, url: searches_path, class: 'form-inline my-2 my-lg-0' do |form| %>
	<%= form.text_field :keywords, class: 'autocomplete form-control mr-sm-2', placeholder: 'Search' %>
	<%= form.submit "Search", class: 'btn btn-outline-success my2 my-sm-0' %>
<% end %>

