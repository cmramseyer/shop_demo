<table class="table">
	<thead>
		<tr>
			<th>Type</th>
			<th></th>
			<th>Overview</th>
			<th>Date</th>
		</tr>
	</thead>
	<tbody>
		<% @records.each do |record| %>
			<tr>
				<td><%= record.type %></td>
				<td><%= record.title %></td>
				<td><%= record.overview %></td>
				<td><%= record.created_at.only_date_format %></td>
			</tr>
		<% end %>
	</tbody>
</table>